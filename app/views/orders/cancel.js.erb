<% if @status == 1 %>
$("#incompleted_orders_link").text("待配送(<%= @incompleted_orders_count > 0 ? @incompleted_orders_count - 1 : 0 %>)")
<% if @incompleted %>
  $("#item-row-<%= @order.id %>").remove();
  $("#item-row-info-<%= @order.id %>").remove();
<% else %>
  $("#item-row-<%= @order.id %>").replaceWith("<%= j render @order %>");
<% end %>
<% elsif @status == -1 %>
console.log("非法操作");
<% end %>