<div class="product">
  <div class="col-md-4">
    <div class="thumbnail">
      <%= image_tag product.image.thumb, alt: product.title %>
      <div class="caption">
        <h3><%= product.title %></h3>
        <p>已经有6人预订</p>
        <p>当前价：<%= product.origin_price %> 元/斤</p>
        <p>最低价：<%= product.low_price %> 元/斤</p>
        <p><a href="#" class="btn btn-primary" role="button">立即预订</a></p>
      </div>
    </div>
  </div>
</div>